#!/bin/sh -e

# $Id: setup,v 1.3 2003/04/25 02:40:07 bsittler Exp $

cd `dirname $0`

if [ ! -x configure ]; then
    echo "Generating configure..."
    autoconf
fi

if [ ! -f kn_config.h.in ]; then
    echo "Generating kn_config.h.in..."
    (cd buildconf && autoheader ../configure.in && cp kn_config.h.in ..)
fi

# End of setup
